{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\usersdb;Database=mobilesdb;Trusted_Connection=True;"
  },
  "Serilog":{
    "MinimumLevel":"Warning",
    "WriteTo": [
      {
        "Name": "Console",
